<mvc:View
	controllerName="sap.ui.demo.cart.controller.MyApproval"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc">
	<Page
		id="page"
		title="My Approval"
		backgroundDesign="Solid">
		<headerContent>
			<Button icon="sap-icon://customer" tooltip="User" press="onUserPress"></Button>
			<Button icon="sap-icon://log" type="Emphasized" tooltip="Logout" press="logOutApp"/>
		</headerContent>
		<content>
			<List id="myProducts" growingThreshold="10" growing="true">
			</List>
		</content>
	</Page>
</mvc:View>
