<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form"
  xmlns:html="http://www.w3.org/1999/xhtml">
  <Dialog>
    <customHeader>
      <Toolbar>
        <ToolbarSpacer width="105px" />
        <Title id="idTitleAbout" text="About" textAlign="Center" />
        <ToolbarSpacer />

      </Toolbar>
    </customHeader>
    <content>
      <VBox alignItems="Center" direction="Column" fitContainer="true" width="100%" height="100%"
        justifyContent="Center" renderType="Div" visible="true" displayInline="false">
        <items>
          <Image id="idImage11" class="sapUiSmallMarginTop sapUiSmallMarginEnd" src="img/soyuz.png"
            width="133px" height="60px" />
          <form:SimpleForm>
            <form:content>
              <Label id="idAboutName" text="Name" />
              <Text text="Mr. Anurag Bajaj" />
              <Label id="idAboutAddress" text="Address" />
              <Text text="EMAAR MGF EPS-FF-073A, Emerald Plaza" />
              <!-- <Label text="" /> -->
              <Text text="Sector 65, Golf course Extension Road," />
              <!-- <Label text="" /> -->
              <Text text="Gurgaon, Haryana-122102" />

              <Label id="idAboutEmail" text="Email" />
              <html:p>
                <Link href="mailto: contact@anubhavtrainings.com" text="contact@soyuztechnologies.com"></Link>
              </html:p>
              <Label id="idAboutContact" text="Phone" />
              <html:p>
                <Link href="tel: +91 8810504396" text="+91 8810504396"></Link>
              </html:p>
              <!-- <Text text="+91 8810504396" /> -->
            </form:content>
          </form:SimpleForm>
        </items>
      </VBox>
    </content>
    <beginButton>
      <Button text="Ok" type="Emphasized" press="onCloseAbout" />
    </beginButton>

  </Dialog>
</core:FragmentDefinition>
