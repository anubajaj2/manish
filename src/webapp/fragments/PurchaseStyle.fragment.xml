        <t:columns>

          <t:Column width="20%">
            <Label text="ITEM CODE" />
            <t:template>
              <Text text="" wrapping="true" />
            </t:template>
          </t:Column>
          <t:Column width="20%">
            <Label text="TAG NO" />
            <t:template>
              <Text text="{dataModel>TagNo}" wrapping="true" />
            </t:template>
          </t:Column>

          <t:Column width="20%">
            <Label text="G WT" />
            <t:template>
