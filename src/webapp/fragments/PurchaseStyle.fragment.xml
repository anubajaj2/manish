            </t:template>
          </t:Column>

          <t:Column width="20%">
            <Label text="Photo" />
            <t:template>
              <Button icon="sap-icon://image-viewer" text="" wrapping="true" />
            </t:template>
          </t:Column>

          <t:Column width="20%">
            <Label text="Remarks" />
            <t:template>
              <Text text="{dataModel>Remark}" wrapping="true" />
            </t:template>
          </t:Column>
		   <t:Column width="20%">
